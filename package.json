{
	"name": "personal-site",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "bun run index.ts",
		"build:prod": "bun run index.prod.ts",
		"clean": "rm -rf dist",
		"dev": "bun run build && bun run serve",
		"format": "biome format --write .",
