		"check": "biome check --write .",
		"serve": "bun run serve.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "1.3.3",
		"@types/marked": "6.0.0"
	},
	"peerDependencies": {
		"typescript": "5.9.3"
	},
	"dependencies": {
		"marked": "17.0.1"
	}
}
